<wpml-config>
	<gutenberg-blocks>
		<gutenberg-block type="uagb/advanced-heading" translate="1">
			<xpath>//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6]</xpath>
			<xpath>//div/p[@class="uagb-desc-text"]</xpath>
			<key name="customUrl"/>
		</gutenberg-block>
		<gutenberg-block type="uagb/post-masonry" translate="1">
			<key name="ctaText"/>
		</gutenberg-block>
		<gutenberg-block type="uagb/post-grid" translate="1">
			<key name="ctaText"/>
		</gutenberg-block>
		<gutenberg-block type="uagb/post-timeline" translate="1">
			<key name="readMoreText"/>
		</gutenberg-block>
		<gutenberg-block type="uagb/post-carousel" translate="1">
			<key name="ctaText"/>
		</gutenberg-block>
		<gutenberg-block type="uagb/info-box" translate="1">
			<xpath label="Title Prefix">//span[@class="uagb-ifb-title-prefix"]</xpath>
			<xpath label="Title">//*[@class="uagb-ifb-title"]</xpath>
			<xpath label="Description">//p[@class="uagb-ifb-desc"]</xpath>
			<xpath label="Button Text">//span[contains(@class, 'uagb-inline-editing')]</xpath>
			<xpath label="Button Link" type="link">//a/@href</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/testimonial" translate="1">
			<xpath>//div[@class="uagb-tm__desc"]</xpath>
			<xpath>//span[@class="uagb-tm__author-name"]</xpath>
			<xpath>//span[@class="uagb-tm__company"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/team" translate="1">
			<xpath>//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6]</xpath>
			<xpath>//span[@class="uagb-team__prefix"]</xpath>
			<xpath>//p[@class="uagb-team__desc"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/icon-list-child" translate="1">
			<xpath>//span[@class="uagb-icon-list__label"]</xpath>
			<xpath type="link">//div[contains(@class, "wp-block-uagb-icon-list-child")]/a/@href</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/restaurant-menu-child" translate="1">
			<xpath>//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6]</xpath>
			<xpath>//div[@class="uagb-rm__desc"]</xpath>
			<xpath>//span[@class="uagb-rm__price"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/content-timeline-child" translate="1">
			<xpath>//div[@class="uagb-timeline__date-new"]/div</xpath>
			<xpath>//div[@class="uagb-timeline__inner-date-new"]</xpath>
			<xpath>//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6]</xpath>
			<xpath>//p[@class="uagb-timeline-desc-content"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/call-to-action" translate="1">
			<xpath>//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6]</xpath>
			<xpath>//p[@class="uagb-cta__desc"]</xpath>
			<xpath type="link">//a[contains(@class,"uagb-cta__block-link")]/@href</xpath>
			<xpath>//span[@class="uagb-cta__link-content-inner"]/span</xpath>
			<xpath>//*[@class="uagb-cta__buttons"]/a</xpath>
			<xpath type="link">//*[@class="uagb-cta__buttons"]/a/@href</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/blockquote" translate="1">
			<xpath>//div[@class="uagb-blockquote__content"]</xpath>
			<xpath>//cite[@class="uagb-blockquote__author"]</xpath>
			<xpath>//span[@class="uagb-blockquote__tweet-label"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/marketing-button" translate="1">
			<xpath type="link">//a[@class="uagb-marketing-btn__link"]/@href</xpath>
			<xpath>//*[@class="uagb-marketing-btn__title"]</xpath>
			<xpath>//p[@class="uagb-marketing-btn__prefix"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/table-of-contents" translate="1">
			<xpath>//div[@class="uagb-toc__title"]</xpath>
			<xpath>//ul[@class="uagb-toc__list"]/li/a</xpath>
			<xpath type="link">//a/@href</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/buttons-child" translate="1">
			<xpath label="Button text">//*[@class="uagb-button__link"]</xpath>
			<xpath label="Button link" type="link">//a/@href</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/wp-search" translate="1">
			<xpath>//input[@class="uagb-search-form__input"]/@title</xpath>
			<xpath>//input[@class="uagb-search-form__input"]/@placeholder</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/inline-notice" translate="1">
			<xpath>//*[@class="uagb-notice-title"]</xpath>
			<xpath>//*[@class="uagb-notice-text"]/p</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/faq-child" translate="1">
			<xpath>//*[@class="uagb-question"]</xpath>
			<xpath>//*[@class="uagb-faq-content"]/span/p</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/how-to" translate="1">
			<xpath>//*[@class="uagb-howto-heading-text"]</xpath>
			<xpath>//*[@class="uagb-howto-desc-text"]</xpath>
			<xpath>//*[@class="uagb-howto-timeNeeded-text"]</xpath>
			<xpath>//*[@class="uagb-howto-timeNeeded-value"]</xpath>
			<xpath>//*[@class="uagb-howto-timeINmin-text"]</xpath>
			<xpath>//*[@class="uagb-howto-estcost-text"]</xpath>
			<xpath>//*[@class="uagb-howto-estcost-value"]</xpath>
			<xpath>//*[@class="uagb-howto-estcost-type"]</xpath>
			<xpath>//*[@class="uagb-howto-req-tools-text"]</xpath>
			<xpath>//*[@class="uagb-tools__label"]</xpath>
			<xpath>//*[@class="uagb-howto-req-materials-text"]</xpath>
			<xpath>//*[@class="uagb-materials__label"]</xpath>
			<xpath>//*[@class="uagb-howto-req-steps-text"]</xpath>
			<xpath>//*[@class="uagb-ifb-title"]</xpath>
			<xpath>//*[@class="uagb-ifb-desc"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/faq" translate="0"/>
		<gutenberg-block type="uagb/section" translate="0"/>
		<gutenberg-block type="uagb/buttons" translate="0"/>
		<gutenberg-block type="uagb/icon-list" translate="0"/>
		<gutenberg-block type="uagb/columns" translate="0"/>
		<gutenberg-block type="uagb/column" translate="0"/>
		<gutenberg-block type="uagb/social-share" translate="0"/>
		<gutenberg-block type="uagb/social-share-child" translate="0"/>
		<gutenberg-block type="uagb/google-map" translate="0"/>
		<gutenberg-block type="uagb/restaurant-menu" translate="0"/>
		<gutenberg-block type="uagb/content-timeline" translate="0"/>
		<gutenberg-block type="uagb/tabs" translate="1">
			<xpath>//span</xpath>
			<xpath>//li[@class="uagb-tab"]/li</xpath>
			<xpath>//div[(@class="uagb-tabs__body")]/div</xpath>
		</gutenberg-block>
		<gutenberg-block type="uagb/forms" translate="1">
			<xpath>//button/div</xpath>
		</gutenberg-block>
	</gutenberg-blocks>
</wpml-config>
