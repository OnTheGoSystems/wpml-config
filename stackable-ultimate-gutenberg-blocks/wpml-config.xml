<wpml-config>
    <gutenberg-blocks>
        <gutenberg-block type="stackable/accordion" translate="0" />
        <gutenberg-block type="stackable/blockquote" translate="0" />
        <gutenberg-block type="stackable/button-group" translate="0" />
        <gutenberg-block type="stackable/button" translate="1">
            <xpath>//*[contains(@class,"stk-button__inner-text")]</xpath>
            <xpath>//*[contains(@class,"stk-block-button")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/call-to-action" translate="1">
            <xpath>//*[contains(@class,"stk-block-call-to-action__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/card" translate="1">
            <xpath>//*[contains(@class,"stk-block-card__image")]/img/@alt</xpath>
            <xpath>//*[contains(@class,"stk-block-card__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/column" translate="1">
            <xpath>//*[contains(@class,"stk-block-column__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/columns" translate="0" />
        <gutenberg-block type="stackable/count-up" translate="1">
            <xpath>//*[contains(@class,"stk-block-count-up__text")]</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/design-library" translate="0" />
        <gutenberg-block type="stackable/divider" translate="0" />
        <gutenberg-block type="stackable/expand" translate="0" />
        <gutenberg-block type="stackable/feature-grid" translate="0" />
        <gutenberg-block type="stackable/feature" translate="0" />
        <gutenberg-block type="stackable/heading" translate="1">
            <xpath>//*[contains(@class,"stk-block-heading__text")]</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/hero" translate="0" />
        <gutenberg-block type="stackable/icon-box" translate="1">
            <xpath>//*[contains(@class, "stk-block-icon-box__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/icon-button" translate="0" />
        <gutenberg-block type="stackable/icon-label" translate="0" />
        <gutenberg-block type="stackable/icon-list" translate="1">
            <xpath>//*[contains(@class, "stk-block-icon-list")]//li</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/icon" translate="1">
            <xpath>//*[contains(@class, "stk--inner-svg")]/svg/@alt</xpath>
            <xpath>//*[contains(@class,"stk-block-icon")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/image-box" translate="1">
            <xpath>//*[contains(@class, "stk-block-image-box__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/image" translate="1">
            <xpath>//*[contains(@class, "stk-img-wrapper")]/img/@alt</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/notification" translate="1">
            <xpath>//*[contains(@class, "stk-block-notification__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/number-box" translate="0" />
        <gutenberg-block type="stackable/posts" translate="1">
            <xpath>//*[contains(@class, "stk-block-posts__readmore")]</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/price" translate="0" />
        <gutenberg-block type="stackable/pricing-box" translate="1">
            <xpath>//*[contains(@class, "stk-block-pricing-box__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/separator" translate="0" />
        <gutenberg-block type="stackable/spacer" translate="0" />
        <gutenberg-block type="stackable/subtitle" translate="1">
            <xpath>//*[contains(@class, "stk-block-subtitle__text")]</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/team-member" translate="1">
            <xpath>//*[contains(@class, "stk-block-team-member__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/testimonial" translate="1">
            <xpath>//*[contains(@class, "stk-block-testimonial__content")]/a/@title</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/text" translate="1">
            <xpath>//*[contains(@class, "stk-block-text__text")]</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/video-popup" translate="1">
            <xpath>//*[contains(@class, "stk-block-video-popup")]/button/@aria-label</xpath>
        </gutenberg-block>
        <gutenberg-block type="stackable/load-more" translate="0" />
        <gutenberg-block type="stackable/pagination" translate="1">
            <xpath>//*[contains(@rel, "prev")]/span</xpath>
            <xpath>//*[contains(@rel, "next")]/span</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/blog-posts" translate="1">
            <key name="readmoreText" />
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/heading" translate="1">
            <xpath>//*[@class="ugb-heading__title"]</xpath>
            <xpath>//*[@class="ugb-heading__subtitle"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/accordion" translate="1">
            <xpath>//*[@class="ugb-accordion__title"]</xpath>
            <xpath>//*[@class="ugb-accordion__content-inner"]/p</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/blockquote" translate="1">
            <xpath>//*[@class="ugb-blockquote__text"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/button" translate="1">
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/count-up" translate="1">
            <xpath>//*[@class="ugb-countup__title"]</xpath>
            <xpath>//*[@class="ugb-countup__counter"]</xpath>
            <xpath>//*[@class="ugb-countup__description"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/card" translate="1">
            <xpath>//*[@class="ugb-card__title"]</xpath>
            <xpath>//*[@class="ugb-card__subtitle"]</xpath>
            <xpath>//*[@class="ugb-card__description"]</xpath>
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/cta" translate="1">
            <xpath>//*[@class="ugb-cta__title"]</xpath>
            <xpath>//*[@class="ugb-cta__description"]</xpath>
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/feature-grid" translate="1">
            <xpath>//*[@class="ugb-feature-grid__title"]</xpath>
            <xpath>//*[@class="ugb-feature-grid__description"]</xpath>
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/expand" translate="1">
            <xpath>//*[@class="ugb-expand__title"]</xpath>
            <xpath>//*[@class="ugb-expand__less-text"]/p</xpath>
            <xpath>//*[@class="ugb-expand__more-text"]/p</xpath>
            <xpath>//*[@class="ugb-expand__more-toggle-text"]</xpath>
            <xpath>//*[@class="ugb-expand__less-toggle-text"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/header" translate="1">
            <xpath>//*[@class="ugb-header__title"]</xpath>
            <xpath>//*[@class="ugb-header__subtitle"]</xpath>
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/image-box" translate="1">
            <xpath>//*[@class="ugb-image-box__subtitle"]</xpath>
            <xpath>//*[@class="ugb-image-box__title"]</xpath>
            <xpath>//*[@class="ugb-image-box__description"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/number-box" translate="1">
            <xpath>//*[@class="ugb-number-box__number"]</xpath>
            <xpath>//*[@class="ugb-number-box__title"]</xpath>
            <xpath>//*[@class="ugb-number-box__description"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/pricing-box" translate="1">
            <xpath>//*[@class="ugb-pricing-box__title"]</xpath>
            <xpath>//*[@class="ugb-pricing-box__price-prefix"]</xpath>
            <xpath>//*[@class="ugb-pricing-box__price"]</xpath>
            <xpath>//*[@class="ugb-pricing-box__price-suffix"]</xpath>
            <xpath>//*[@class="ugb-pricing-box__subprice"]</xpath>
            <xpath>//*[@class="ugb-pricing-box__description"]</xpath>
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/icon-list" translate="1">
            <xpath>//*[contains(@class, "wp-block-ugb-icon-list")]//li</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/notification" translate="1">
            <xpath>//*[@class="ugb-notification__title"]</xpath>
            <xpath>//*[@class="ugb-notification__description"]</xpath>
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/team-member" translate="1">
            <xpath>//*[@class="ugb-team-member__name"]</xpath>
            <xpath>//*[@class="ugb-team-member__position"]</xpath>
            <xpath>//*[@class="ugb-team-member__description"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/testimonial" translate="1">
            <xpath>//*[@class="ugb-testimonial__body"]</xpath>
            <xpath>//*[@class="ugb-testimonial__name"]</xpath>
            <xpath>//*[@class="ugb-testimonial__position"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/text" translate="1">
            <xpath>//*[@class="ugb-text__text"]/p</xpath>
            <xpath>//*[@class="ugb-text__title"]</xpath>
            <xpath>//*[@class="ugb-text__subtitle"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/feature" translate="1">
            <xpath>//*[@class="ugb-feature__title"]</xpath>
            <xpath>//*[@class="ugb-feature__description"]</xpath>
            <xpath>//a[contains(@class, "ugb-button")]/@href</xpath>
            <xpath>//*[@class="ugb-button--inner"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/icon" translate="1">
            <xpath>//a[@class="ugb-icon__icon"]/@href</xpath>
            <xpath>//*[@class="ugb-icon__title"]</xpath>
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/video-popup" translate="1">
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/columns" translate="1">
            <xpath>//*[contains(@class,"ugb-block-title")]</xpath>
            <xpath>//*[@class="ugb-block-description"]</xpath>
        </gutenberg-block>
        <gutenberg-block type="ugb/separator" translate="0" />
        <gutenberg-block type="ugb/spacer" translate="0" />
        <gutenberg-block type="ugb/container" translate="0" />
        <gutenberg-block type="ugb/divider" translate="0" />
        <gutenberg-block type="ugb/column" translate="0" />
        <gutenberg-block type="ugb/design-library" translate="0" />
    </gutenberg-blocks>
</wpml-config>
