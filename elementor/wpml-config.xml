<wpml-config>
   <custom-fields>
        <custom-field action="copy-once">_elementor_page_settings</custom-field>
        <custom-field action="copy">_elementor_edit_mode</custom-field>
        <custom-field action="copy">_elementor_template_type</custom-field>
        <custom-field action="copy">_elementor_version</custom-field>
        <custom-field action="copy-once">_elementor_data</custom-field>
        <custom-field action="ignore">_elementor_css</custom-field>
        <custom-field action="copy">_elementor_edit_mode</custom-field>
        <custom-field action="copy-once">_elementor_popup_display_settings</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1" display-as-translated="1">elementor_library</custom-type>
        <custom-type translate="1" display-as-translated="1">e-landing-page</custom-type>
    </custom-types>
    <taxonomies>
        <taxonomy translate="0">elementor_library_type</taxonomy>
    </taxonomies>
    <elementor-widgets>
        <widget name="form">
            <fields>
                <field type="Form: name" editor_type="LINE">form_name</field>
                <field type="Form: Button text" editor_type="LINE">button_text</field>
                <field type="Form: Email subject" editor_type="LINE">email_subject</field>
                <field type="Form: Email from name" editor_type="LINE">email_from_name</field>
                <field type="Form: Email Content" editor_type="AREA">email_content</field>
                <field type="Form: Email subject 2" editor_type="LINE">email_subject_2</field>
                <field type="Form: Email Content 2" editor_type="AREA">email_content_2</field>
                <field type="Form: Success message" editor_type="LINE">success_message</field>
                <field type="Form: Error message" editor_type="LINE">error_message</field>
                <field type="Form: Required message" editor_type="LINE">required_message</field>
                <field type="Form: Invalid message" editor_type="LINE">invalid_message</field>
                <field type="Form: Required message" editor_type="LINE">required_field_message</field>
                <field type="Form: Redirect to URL" editor_type="LINE">redirect_to</field>
                <field type="Form: MailChimp Tags" editor_type="LINE">mailchimp_tags</field>
                <field type="Form: Previous Button" editor_type="LINE">step_previous_label</field>
                <field type="Form: Step Next Label" editor_type="LINE">step_next_label</field>		    
            </fields>
            <fields-in-item items_of="form_fields">
                <field type="Form: Field label" editor_type="LINE">field_label</field>
                <field type="Form: Field placeholder" editor_type="LINE">placeholder</field>
                <field type="Form: Field HTML" editor_type="AREA">field_html</field>
                <field type="Form: Acceptance Text" editor_type="LINE">acceptance_text</field>
                <field type="Form: Checkbox Options" editor_type="AREA">field_options</field>
                <field type="Form: Step Next Label" editor_type="LINE">step_next_label</field>
                <field type="Form: Step Previous Label" editor_type="LINE">step_previous_label</field>
                <field type="Form: Previous Button" editor_type="LINE">previous_button</field>
                <field type="Form: Next Button" editor_type="LINE">next_button</field>
            </fields-in-item>
        </widget>
        <widget name="shortcode">
            <fields>
                <field type="Shortcode">shortcode</field>
            </fields>
        </widget>
        <widget name="wc-add-to-cart">
            <fields>
                <field type="Button Text" editor_type="LINE">text</field>
            </fields>
        </widget>
        <widget name="wp-widget-recent-posts">
            <fields>
                <field type="Title" editor_type="LINE">wp>title</field>
            </fields>
        </widget>
        <widget name="wp-widget-text">
            <fields>
                <field type="Title" editor_type="LINE">wp>title</field>
                <field type="Text" editor_type="VISUAL">wp>text</field>
            </fields>
        </widget>
	    <widget name="media-carousel">
            <fields-in-item items_of="slides">
                <field type="Slide Video Link" editor_type="LINE" key_of="video">url</field>
            </fields-in-item>
        </widget>
        <widget name="media-carousel">
            <fields-in-item items_of="slides">
                <field type="Slide Image Link" editor_type="LINE" key_of="image_link_to">url</field>
            </fields-in-item>
        </widget>        
        <widget name="text-path">
            <fields>
                <field type="Text path">text</field>
                <field type="Text path link" editor_type="LINK">link>url</field>
            </fields>
        </widget>
        <widget name="star-rating">
            <fields>
                <field type="Title" editor_type="LINE">title</field>
            </fields>
        </widget>
        <widget name="video-playlist">
            <fields>
                <field type="Video Playlist: Title">playlist_title</field>
                <field type="Video Playlist: Tab title 1">inner_tab_title_1</field>
                <field type="Video Playlist: Tab title 2">inner_tab_title_2</field>
                <field type="Video Playlist: Show more">inner_tab_label_show_more</field>
                <field type="Video Playlist: Show less">inner_tab_label_show_less</field>
            </fields>
            <fields-in-item items_of="tabs">
                <field type="Video Playlist: Title">title</field>
                <field type="Video Playlist: Youtube URL" editor_type="LINK">youtube_url</field>
                <field type="Video Playlist: External URL" editor_type="LINK">external_url>url</field>
                <field type="Video Playlist: Tab 1 content" editor_type="VISUAL">inner_tab_content_1</field>
                <field type="Video Playlist: Tab 2 content" editor_type="VISUAL">inner_tab_content_2</field>
            </fields-in-item>
        </widget>
        <widget name="hotspot">
            <fields-in-item items_of="hotspot">
                <field type="Hotspot: Label">hotspot_label</field>
                <field type="Hotspot: URL" editor_type="LINK">hotspot_link>url</field>
                <field type="Hotspot: Content" editor_type="VISUAL">hotspot_tooltip_content</field>
            </fields-in-item>
        </widget>
        <widget name="image-carousel">
            <fields>
                <field type="Heading: Link URL" editor_type="LINK">link>url</field>
            </fields>
        </widget>
	<widget name="woocommerce-cart">
            <conditions>
                <condition key="widgetType">woocommerce-cart</condition>
            </conditions>
            <fields>
                <field type="Cart: Update button text" editor_type="LINE">update_cart_button_text</field>
                <field type="Cart: Apply coupon botton text" editor_type="LINE">apply_coupon_button_text</field>
                <field type="Cart: Totals section title" editor_type="LINE">totals_section_title</field>
                <field type="Cart: Update shipping button text" editor_type="LINE">update_shipping_button_text</field>
                <field type="Cart: Checkout button text" editor_type="LINE">checkout_button_text</field>
            </fields>
        </widget>
        <widget name="woocommerce-checkout-page">
            <conditions>
                <condition key="widgetType">woocommerce-checkout-page</condition>
            </conditions>
            <fields>
                <field type="Checkout: Billing details section title" editor_type="LINE">billing_details_section_title</field>
                <field type="Checkout: Billing forms items" editor_type="LINE">title_field</field>
                <field type="Checkout: Shipping details section title" editor_type="LINE">shipping_details_section_title</field>
                <field type="Checkout: Create account text" editor_type="LINE">create_account_text</field>
                <field type="Checkout: Order summary section title" editor_type="LINE">order_summary_section_title</field>
                <field type="Checkout: Terms and conditions message text" editor_type="LINE">terms_conditions_message_text</field>
                <field type="Checkout: Terms and conditions link text" editor_type="LINE">terms_conditions_link_text</field>
                <field type="Checkout: Coupon title text" editor_type="LINE">coupon_section_title_text</field>
                <field type="Checkout: Coupon link text" editor_type="LINE">coupon_section_title_link_text</field>
            </fields>
            <fields-in-item items_of="billing_details_form_fields">
                <field type="Checkout: Billing forms items label" editor_type="LINE">label</field>
                <field type="Checkout: Billing forms items placeholder" editor_type="LINE">placeholder</field>
            </fields-in-item>
            <fields-in-item items_of="shipping_details_form_fields">
                <field type="Checkout: Shipping forms items label" editor_type="LINE">label</field>
                <field type="Checkout: Shipping forms items placeholder" editor_type="LINE">placeholder</field>
            </fields-in-item>
            <fields-in-item items_of="additional_information_form_fields">
                <field type="Checkout: Additional information forms items label" editor_type="LINE">label</field>
                <field type="Checkout: Additional information forms items placeholder" editor_type="LINE">placeholder</field>
            </fields-in-item>
        </widget>
    </elementor-widgets>
</wpml-config>
