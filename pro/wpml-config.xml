        <field type="Custom Headline: Headline">content</field>
      </fields>
    </widget>
    <widget name="classic:image">
      <fields>
        <field type="Image: Alternative Text">alt</field>
        <field type="Image: Link" editor_type="LINK">href</field>
        <field type="Image: Link Title">href_title</field>
